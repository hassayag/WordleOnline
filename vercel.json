{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "wordle-api/build/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["wordle-api/build/**"] }
      },
      {
        "src": "wordle-ui/build/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["wordle-ui/build/**"] }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "wordle-api/build/index.js"
      },
      {
        "src": "/",
        "dest": "wordle-ui/build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "wordle-ui/build/$1"
      }
    ]
  }